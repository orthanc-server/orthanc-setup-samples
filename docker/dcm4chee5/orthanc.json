    "Servers" : {
      "dcm4chee" : {
        "Url" : "https://arc:8443/dcm4chee-arc/aets/DCM4CHEE/rs/",
        //"Username" : "username",
        //"Password" : "password",
        "HasDelete" : false
      }
    }
  }
}
